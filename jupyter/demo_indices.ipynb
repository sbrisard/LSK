   "outputs": [],
   "source": [
    "i, j, m = sympy.symbols(\"i j m\")\n",
    "ξ = sympy.Function(r\"\\xi\")\n",
    "v = sympy.Function(\"v\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "eafa89e1-a786-4aea-b717-9de324da89a9",
   "metadata": {},
   "outputs": [],
   "source": [
    "a = sympy.summation(ξ(i) * v(i), (i, 1, m))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "a3455c2a-4bc5-4757-898a-0345583b0295",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$\\displaystyle \\sum_{i=1}^{m} \\xi{\\left(i \\right)} v{\\left(i \\right)}$"
      ],
      "text/plain": [
       "Sum(\\xi(i)*v(i), (i, 1, m))"
      ]
     },
     "execution_count": 4,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "a"
   ]
  },
